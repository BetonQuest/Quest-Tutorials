# EditorConfig is awesome: https://EditorConfig.org
root = true

[*]
charset = utf-8
insert_final_newline = true
indent_style = space
indent_size = 2
#The line ending lf is only checked in the build pipeline. Don't remove this and the following line.
#end_of_line = lf
[/.idea/**]
insert_final_newline = unset

[LICENSE]
charset = unset
insert_final_newline = unset
indent_style = unset
indent_size = unset
